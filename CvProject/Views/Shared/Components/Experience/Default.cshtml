@model IEnumerable<EntityLayer.Concrete.Experience> 
						<section class="resume-section spad pt-0">
							<div class="section-title">
								<h2>Deneyimler</h2>
							</div>
							<ul class="resume-list">
								@foreach(var item in Model){
								<li>
									<h2>@item.ExperienceYear</h2>
									<h3>@item.ExperienceName</h3>
									<h4>@item.ExperienceTask</h4>
									<p>@item.ExperienceDescription </p>
								</li>
								}
							</ul>
						</section>