@model IEnumerable<EntityLayer.Concrete.Education>  
<section class="resume-section">
							<div class="section-title">
								<h2>Eğitim</h2>
							</div>
							<ul class="resume-list">
								@foreach(var item in Model){
								<li>
									<h2>@item.EducationYear</h2>
									<h3>@item.EducationDegree</h3>
									<h4>@item.EducationName</h4>
									<p>@item.EducationSection </p>
								</li>
								}
							</ul>
						</section>